#ifndef MENU_H_
#define MENU_H_

#include "stm32l4xx_hal.h"


#define ParamNb
typedef enum
{
    Voltage,
    Current,
    Power
} MenuParamTypedef;

typedef struct
{
	float Val;
	float MinVal;
	float MaxVal;
	char ValDigitNb;
} MenuParamValTypedef;


void Menu_LoadParam(MenuParamTypedef Param,  MenuParamValTypedef * ParamValues);
void Menu_Init(UART_HandleTypeDef * UsedUSART);



#endif /* INC_MYANALOG_H_ */
