float Val; // à mettre en local après debug
void StringFct_Float2Str(float Value,char * Str, char DgitNb, char Frac)
{

	switch (Frac)
	{
	case 0:Val=Value;break;
	case 1:Val=10.0*Value;break;
	case 2:Val=100.0*Value;break;
	case 3:Val=1000.0*Value;break;
	default : while(1);
	}



}
